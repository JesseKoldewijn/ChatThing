{
	"name": "chatthing",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "vite start",
		"type-check": "tsc -b --noEmit",
		"lint": "oxlint .",
		"knip": "knip",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage && node scripts/display-coverage.js unit",
		"test:coverage:silent": "vitest run --coverage",
		"test:integration": "vitest --config vitest.config.integration.ts",
		"test:integration:run": "vitest run --config vitest.config.integration.ts",
		"test:integration:coverage": "vitest run --config vitest.config.integration.ts --coverage && node scripts/display-coverage.js integration",
		"test:integration:coverage:silent": "vitest run --config vitest.config.integration.ts --coverage",
		"test:e2e": "vitest --config vitest.config.e2e.ts",
		"test:e2e:run": "vitest run --config vitest.config.e2e.ts",
		"test:e2e:coverage": "vitest run --config vitest.config.e2e.ts --coverage && node scripts/display-coverage.js e2e",
		"test:e2e:coverage:silent": "vitest run --config vitest.config.e2e.ts --coverage",
		"test:all": "$npm_execpath test:run && $npm_execpath test:integration:run && $npm_execpath test:e2e:run",
		"test:all:coverage": "$npm_execpath test:coverage:silent && $npm_execpath test:integration:coverage:silent && $npm_execpath test:e2e:coverage:silent && node scripts/display-coverage.js all",
		"test:badges": "node scripts/generate-badges.js"
	},
	"dependencies": {
		"@ai-sdk/google": "^3.0.6",
		"@built-in-ai/core": "^3.0.1",
		"@nanostores/react": "^1.0.0",
		"@openrouter/ai-sdk-provider": "^1.5.4",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-router": "^1.141.6",
		"@tanstack/react-start": "^1.141.7",
		"@tanstack/react-table": "^8.21.3",
		"ai": "^6.0.27",
		"ai-sdk-ollama": "^3.1.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.562.0",
		"nanostores": "^1.1.0",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-is": "^19.2.3",
		"react-markdown": "^10.1.0",
		"react-syntax-highlighter": "^16.1.0",
		"recharts": "^3.5.1",
		"remark-gfm": "^4.0.1",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.0",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/react-is": "^19.2.0",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser": "^4.0.15",
		"@vitest/browser-playwright": "^4.0.15",
		"@vitest/coverage-v8": "^4.0.15",
		"babel-plugin-react-compiler": "^1.0.0",
		"fake-indexeddb": "^6.2.5",
		"happy-dom": "^20.0.11",
		"knip": "^5.80.2",
		"nitro": "npm:nitro-nightly@latest",
		"oxlint": "^1.33.0",
		"playwright": "^1.57.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@^7.3.0",
		"vite-plugin-pwa": "^1.2.0",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.15",
		"vitest-axe": "^0.1.0"
	},
	"packageManager": "yarn@4.12.0",
	"volta": {
		"node": "25.2.1",
		"yarn": "4.12.0"
	}
}
